package PracticeJava;

class  ArrayList<T,E>
{
	Object arry[];
	int capacity;
	int s;
	
	
	ArrayList()
	{
		capacity=10;
		  s=0;
		  arry=new Object[capacity];
		  
		
	}
	
	void Add(T obj)
	{
		
		 arry[s++]=(T)obj;
		 
	}
	
	Object Get(int index)
	{
		return arry[index];
		
	}
	int Size()
	{
	return 	s;
	}
}


public class TEst {

	public static void main(String[] args) {
		
		ArrayList a=new ArrayList ();

		System.out.println(a.Size());

		a.Add(10);
		a.Add("Sanjay");
		a.Add(3.3);
		
		

		
		

	}

}
