package objectClass;
class Square{
	int no;
	public void setValue(int no) {
		this.no=no;
	}
	public int getSquare() {
		return no*no;
	}
	
}
public class CloneMethod {

	public static void main(String[] args) {
		Square sq=new Square();
		sq.setValue(10);
		Square s1=sq;
		int square=s1.getSquare();
		System.out.println("Square is "+square);

	}

}
